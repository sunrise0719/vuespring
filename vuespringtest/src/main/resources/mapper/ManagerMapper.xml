<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.vuespringtest.mapper.ManagerMapper">
    <select id="getManagerByManager" parameterType="Manager" resultType="Manager">
        select * from manager WHERE managerName=#{managerName} and managerPwd=#{managerPwd}
    </select>
</mapper>